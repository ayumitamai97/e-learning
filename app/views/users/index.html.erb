<h1>Users</h1>

<p class="notice"><%= notice %></p>
<p class="alert"><%= alert %></p>

<table>
  <tr>
    <th>Avator</th>
    <th>Name</th>
    <th>Email</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%# user.avator %></td>
      <td><%# user.name %></td>
      <td><%= user.email %></td>
    </tr>
  <% end %>
</table>
